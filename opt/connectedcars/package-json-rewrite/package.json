{
  "name": "package-json-rewrite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:connectedcars/docker-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^23.6.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testRegex": ".+\\.test\\.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/node_modules/**",
      "!**/types/**"
    ],
    "moduleFileExtensions": [
      "js",
      "node"
    ]
  }
}
